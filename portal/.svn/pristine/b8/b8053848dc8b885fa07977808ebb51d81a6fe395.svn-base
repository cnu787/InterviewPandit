         <xsd:element minOccurs="0" ref="purpose"/>
         <xsd:element maxOccurs="unbounded" minOccurs="0" ref="taxonpath"/>
         <xsd:element minOccurs="0" ref="description"/>
         <xsd:element maxOccurs="unbounded" minOccurs="0" ref="keyword"/>
         <xsd:group ref="grp.any"/>
      </xsd:sequence>
