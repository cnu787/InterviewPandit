   <!-- ******************* -->
   <!-- ** Complex Types ** -->
   <!-- ******************* -->

   <xsd:complexType name="aggregationlevelType">
      <xsd:sequence>
         <xsd:element ref="source"/>
         <xsd:element ref="value"/>
      </xsd:sequence>
   </xsd:complexType>
   
   <xsd:complexType mixed="true" name="annotationType">
      <xsd:sequence>
         <xsd:element minOccurs="0" ref="person"/>
         <xsd:element minOccurs="0" ref="date"/>
         <xsd:element minOccurs="0" ref="description"/>
         <xsd:group ref="grp.any"/>
      </xsd:sequence>
   </xsd:complexType>
   
   <xsd:complexType mixed="true" name="catalogentryType">
      <xsd:sequence>
         <xsd:element ref="catalog"/>
