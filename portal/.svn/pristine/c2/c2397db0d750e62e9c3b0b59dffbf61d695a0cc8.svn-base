* { 
	margin: 0; 
	padding: 0; 
}
body { 
	//font: 14px/1.4 Georgia, Serif; 
        font-family: SEGOEUIL;
} *
p {
	margin: 0; 
} 

.ddset { width:950px; margin:10px auto 10px auto; padding:0px;  }

.varcase {
    width:220px; height:67px; background-image: url(../images/buckets.png); background-position:0px 0px; background-repeat: no-repeat; text-align:center; 
    
}
.varcase .text {
    width:180px; margin-left:20px; height:auto; display:table; padding: 0px 10px; font-size: 12px; color: #333; text-align:center; font-family:Avenir Next LT Pro Regular;    
}

.varcase .varc, .varcase .varcc, .varcase .var { width:40px; height:40px; line-height:40px; vertical-align: middle; margin-top:-5px; position:absolute; margin-left:-5px; background-image: url(../images/buckets.png); background-position:0px -134px; background-repeat: no-repeat; color:#999;  font-family:Avenir Next LT Pro Regular; font-size:14px; font-weight:700; text-align: center; }
.varcase .varcc { background:none; }
.varcase .var { margin:0px; position:relative; background-position: -40px -134px; color:#fff; }

.varcase.dragged { background-position:0px -67px;  }
.varcase.dragged .text { color:#999; }

.buckets { width:950px; height:172px; background:url(../images/buckets.png) 0px -174px no-repeat; display:block; margin:42px auto; }
.buckets .btable { width:950px; height:208px; margin-top: -50px; }
.optdiv { width:230px; height:95px; margin-bottom:60px; padding-top:50px; text-align: center; }
#optdiv_1 { margin-left:-35px; }
#optdiv_2 { margin-right:-35px; } 
.optcase {
    width:40px; height:40px; margin:1px 5px;
}

.draggable { cursor:move } .droppable { }
  
 

#prr { z-index:1000110; background:#fff;  position: absolute; width: 100%; height: 100%; display: none; left:0px; top:0px; }

#btn_submit { margin-bottom:60px; clear:both; background-image:url(../images/submit_on.png); width:86px; height:20px; border:none; opacity:1; cursor:pointer; color:#ffffff; font-weight:bold; padding:10px 32px 10px 13px; font-size:15px; display:inline-block; text-decoration:none; }
#btn_submit:hover {  color:#000000; }
#btn_submit.disabled,#btn_submit.disabled:hover { opacity:0.5; cursor:default; color:#ffffff; }
#lower_text{ width:960px; height: 46px; text-align: center; font-size: 13pt; margin-top: -48px; font-family:Avenir Next LT Pro Regular; background-color: #f2982e; line-height: 1.3; padding:3px; }