/* Custom CSS styles ------------- */
/* Header CSS */
@font-face {
	font-family: 'SEGOEUIL';
	src: url('../fonts/SEGOEUIL.eot');
	src: local('SEGOEUIL'), url('../fonts/SEGOEUIL.woff') format('woff'), url('../fonts/SEGOEUIL.ttf') format('truetype');
}

@font-face {
font-family: 'Avenir Next LT Pro Regular';
font-style: normal;
font-weight: normal;
src: local('Avenir Next LT Pro Regular'), url('../../content/shared/fonts/AvenirNextLTPro-Regular.woff') format('woff');
}


.header-container
{
    width: 100%;
    background-color: white;
    padding-right: 0px;
}

.header-menu
{
    float:left; 
    height: 60px; 
    margin-right: 5px; 
    cursor: pointer;
}

.header-title
{
    line-height: 30px; 
    /*color: #00569d !important;  */
    cursor: text;
}

@media screen and (max-width: 786px) 
{
    .header-title
    {
        line-height: 30px; 
        color: #00569d !important; 
        cursor: text;
        font-size: 16px;
    }
}

.img-resource
{
    height: 65px !important;
    width: 65px !important;
    margin-top: -5px;
    padding: 0px !important;
    background: url(../images/resource.png);
}

.toplnk { 
    width:65px !important; height:50px !important; margin:0px !important; padding:0px !important; display:block;
    background-image:url(../images/top-right-icons.png); background-repeat:no-repeat;
}
.toplnk.resource { background-position: 0px 0px; } .toplnk.resource:hover, .toplnk.resource.selected { background-position: -65px 0px; }
.toplnk.glossary { background-position: 0px -60px; } .toplnk.glossary:hover, .toplnk.glossary.selected { background-position: -65px -60px; }
.toplnk.help { background-position: 0px -120px; } .toplnk.help:hover, .toplnk.help.selected { background-position: -65px -120px; }
.toplnk.close { background-position: 0px -180px; opacity:1; height:60px !important } .toplnk.close:hover, .toplnk.close.selected { background-position: -65px -180px; }

.mainmenu{ 
    width:300px; min-width: 200px; height:680px;
    background:url(../images/mm-bg.png);
    position:absolute; 
    top: 0px;  
    left:-300px;
    z-index:10000020;
}

.main-border{
    background: none !important;
    border-radius: 0px !important;
}

.top-menu-header{
    background-image: url('../images/header.png') !important;
    height: 62px !important;
    border-top: none !important;
}

.top-menu{
    float: left; 
    margin-top: 10px !important;
}

.top-menu-span{
    font-size: 19px; 
    padding-left: 10px; 
    margin-top: 7px; 
    display: block; 
    float: left;
    color: white
}

.top-menu-close{
    float:right;
    margin-top: 7%;
    cursor: pointer;
}

a.list-group-item-success{
    color: #a1a4ab;
    margin-top: 0px;
    border-bottom: solid 1px #3b4453 !important;
    border-top: solid 1px #3b4453 !important;
    font-size: 13px !important;
}

a.list-group-item-success-menu{
    color: #a1a4ab;
    margin-top: -2px;
    border-bottom: solid 1px #3b4453 !important;
    border-top: solid 1px #3b4453 !important;
}

a.list-group-item-success:hover{
    color: white;
}

.list-group-item{
    background-image:url('../images/mm-m-bg.png');
    border: none !important;
    border-top-left-radius: 0px !important;
    border-top-right-radius: 0px !important;
    padding:7px 15px !important;
}

.subdone 
{
color: #78b0cb !important;
}
.item
{
    float:right;
    font-size: 20px;
    margin-top: -3px;
    margin-right: -2px;
    color: #e36f29;
}

.item-a
{
    padding: 0px 0px 0px 20px !important;
    line-height: 30px !important;
}

.item-nav
{
    padding-left: 5px;
    font-size: 13px !important;
}

.item-ul
{
    padding-left: 15px;
    float: left;
    margin-bottom: 0px !important;
    font-size: 20px;
    background:url(../images/menu-lsn-bg.png) 0 0 no-repeat;
}

.opa A:first-child UL { padding-left: 15px;
    float: left;
    margin-bottom: 0px !important;
    font-size: 20px;
    background:url(../images/menu-lsn-bg.png) 0 15px no-repeat;
}

.opa A:last-child UL { padding-left: 15px;
    float: left;
    margin-bottom: 0px !important;
    font-size: 20px;
    background:url(../images/menu-lsn-bg.png) 0 -15px no-repeat;
}

.item-ul li
{
    //margin-top: -1px;
    margin-left: 2px;
}

.visited-text
{
    color: #a1a4ab !important;
}

.visited-li
{
    color: #1e1a13 !important;
}

@media screen and (-ms-high-contrast: active), (-ms-high-contrast: none) {
    .item-ul li
    {
        margin-left: -3px;
    }

    .current-li
    {
        content: url('../images/current.png');
        margin-left: 1px !important;
    }
}

@-moz-document url-prefix() {
    .item-ul li
    {
        margin-left: 0px;
    }
}

.current-module
{
    color: #fff !important;
}

.current-li:after
{
    content: url('../images/current.png');
    margin-left: -23px;
}

.current-text
{
    color: white !important;
}

.list-submenu
{
    opacity: 0.8; 
    color: white !important;
    border: none !important;
    margin-bottom: 0px !important;
    border-top-left-radius: 0px !important;
    border-top-right-radius: 0px !important;
    border-bottom-right-radius: 0px !important;
    border-bottom-left-radius: 0px !important;
}

