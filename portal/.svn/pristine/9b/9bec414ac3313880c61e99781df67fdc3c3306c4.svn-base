/* Footer CSS*/

.footer-container
{
    width: 100%;
    padding-right: 0px !important;
}

.source-image {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
}

.footer-img-logo
{
    background: url('../images/logo.png') no-repeat;
    float: left;
    height: 50px;
    width: 155px;
    margin-top: 4px;
}

.footer-img-next
{
    background: url('../images/btn_next_active.png') no-repeat;
    height: 50px;
    width: 60px;
    cursor: pointer;
}

.footer-img-next:hover
{
    background: url('../images/btn_next_active.png') no-repeat;
    height: 50px;
    width: 60px;
    cursor: pointer;
}

.footer-img-next-disabled
{
    background: url('../images/btn_next_inactive.png') no-repeat;
    height: 50px;
    width: 60px;
    cursor: default;
}

.footer-img-prev
{
    background: url('../images/btn_prev_active.png') no-repeat;
    height: 50px;
    width: 60px;
    cursor: pointer;
    //border-left: solid 1px grey; 
}

.footer-img-prev-disabled
{
    background: url('../images/btn_prev_inactive.png') no-repeat;
    height: 50px;
    width: 60px;
    cursor: default;
}

@media screen and (max-width: 1520px) 
{
    .custom-navbar-nav
    {
        float: left !important;
    }
    .custom-navbar-nav1
    {
        float: right !important;
    }
}

.footer-height
{
    height: 50px;
}

.footer-img-height
{
    height: 50px;
    float: right;
}

.footer-li-count
{
    width:100px;
    //border-left: solid 1px grey;
    padding: 15px 10px 15px 25px;
    color: white;
}

/* Pop-up height */

.popup-height
{
    overflow-y: auto;
    height: 500px;
    border-bottom-right-radius: 4px;
    padding: 0px;
}

.popup-border
{
    border-radius: 5px;
    border: solid 5px rgba(0, 0, 0, .3);
}

.popup-content 
{
    width: 100%;
    margin: 0px auto;
    margin-left:0px;
}

.popup-embed-container 
{
    height: 0;
    width: 100%;
    padding-bottom:470px;
    overflow: hidden;
    position: relative;
    top:0px;
}
			
.popup-embed-container iframe 
{
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
}

.popup-close-margin
{
    margin-left: 30%;
    margin-top: 20%;
}

.popup-close-width
{
    width: 400px !important;
}



.footer {
  position: absolute;
  bottom: 0;
  width: 100%;
  /* Set the fixed height of the footer here */
  height: 50px;
  //background-color: #f5f5f5;
  background-color: #0095da;
  z-index: 100001; 
}


/* Custom page CSS
-------------------------------------------------- */
/* Not required for template or sticky footer method. */

body > .container {
  padding: 60px 0px 0;
}
.container .text-muted {
  margin: 20px 0;
}

.footer > .container {
  padding-right: 15px;
  padding-left: 15px;
}

.list-group-item-success .item.plus { display:block; }
.list-group-item-success .item.minus { display:none; }

.list-group-item-success[aria-expanded="true"] .item.plus { display:none; }
.list-group-item-success[aria-expanded="true"] .item.minus { display:block; }

/* Audio related css */
.audio-container {
    height: 50px !important; border: none !important; background-color: #0095da !important;
}

.audio-controls {
    height: 50px !important; border: none !important; background-color: #0095da !important;
}

.audio-controls {
    width: 30px !important; padding: 5px 10px 0 !important;
}

.audio-progress {
    width: 68% !important; top: 25px !important; height: 1px !important; left: 40px !important;
}

.audio-volume-controls {
    top: 21px !important; width: 20px !important; left: 415px !important;
}

.audio-seek-bar {
    cursor: default !important;
}

.audio-play
{
    margin-top: 11px !important;
    width: 20px !important;
    height: 20px !important;
}

.audio-mute
{
    width: 35px !important;
    height: 26px !important;
    margin-top: -12px !important;
}

.audio-text
{
    height: 30px;
    width: 32px;
    margin-left: 54px;
    margin-top: -37px;
    background: url('../images/transcript_on.png') no-repeat;
    cursor: pointer;
}

.audio-text-off
{
    height: 30px;
    width: 32px;
    margin-left: 54px;
    margin-top: -37px;
    background: url('../images/transcript_off.png') no-repeat;
    cursor: pointer;
}

.audio-hr
{
    height: 30px;
    width: 2px;
    margin-left: 399px;
    margin-top: -34px;
    background: url('../images/audio_hr.png') no-repeat;
}

.audio-hr1
{
    height: 30px;
    width: 2px;
    margin-left: 38px;
    margin-top: -4px;
    background: url('../images/audio_hr.png') no-repeat;
}

.audio-subtitle
{
    background: url(../images/mm-bg.png);
    opacity: 0.9;
    height: 70px;
    width: 100%;
    position: absolute;
    bottom: 51px;
    z-index: 1000;
    color: white;
    display: none;
    overflow-y: auto;
    padding:10px;
}

.audio-transcript
{ 
    font-family: Avenir Next LT Pro Regular;
    line-height:22px;
}

.preloaderbg { width:100%; height:100%; background:#dedede; opacity:0.8; position:absolute; z-index: 1000; display:None; }
.preloaderdiv { width:100%; height:100%; background:url(../images/preloader.gif) center no-repeat; position:absolute; z-index: 1001; display:None; }

@media screen and (max-width: 690px)
{
    .zoom
    {
        zoom: 65%;
    }
    
    .audio-progress {
        height: 2px !important;
    }
}

@media screen and (max-width: 766px) and (min-width: 690px)
{
    .zoom
    {
        zoom: 75%;
    }
    
    .audio-progress {
        height: 2px !important;
    }
}

@media screen and (max-width: 830px)  and (min-width: 766px) 
{
    .zoom
    {
        zoom: 83%;
    }
    
    .audio-progress {
        height: 2px !important;
    }
}

@media screen and (min-width: 831px) and (max-width: 920px)
{
    .zoom
    {
        zoom: 90%;
    }
    
    .audio-progress {
        width: 68% !important; top: 25px !important; height: 2px !important; left: 40px !important;
    }
}

@media screen and (min-width: 1100px) and (max-width: 1220px)
{
    .audio-container-margin
    {
        margin-right: 100px;
    }
}

@media screen and (min-width: 1221px) and (max-width: 1520px)
{
    .audio-container-margin
    {
        margin-right: 200px;
    }
}

.fontpreloader { position:absolute; left:-100px; top:-100px; width:1px; height:1px; font-size:1px; font-family: Avenir Next LT Pro Regular; visibility:hidden;}
.fontpreloader1 { position:absolute; left:-100px; top:-100px; width:1px; height:1px; font-size:1px; font-family: SEGOEUIL; visibility:hidden;}

#glow1 { position:absolute; margin-top:-60px; right:10px; z-index:100001; display: none; }