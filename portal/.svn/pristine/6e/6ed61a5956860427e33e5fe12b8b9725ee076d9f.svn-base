.animate { display:none;}  
#iformation_container{ font-style: normal; font-variant: normal; font-weight: normal; font-size: 12pt; margin-top: 15px; }
#message_text{ font-style: normal; font-variant: normal; font-weight: normal; font-size: 14pt; margin-top:10px;  }
#instruction_text{  font-style: normal; font-variant: normal; font-weight: normal; font-size: 12pt; margin-top:325px; display: none; width: 750px;  }
#question_container { margin-top: 20px; width: 750px;}
#button_container{ margin: 10px 200px;}
#select_inst{ margin-top: 10px; font-style: normal; font-variant: normal; font-weight: normal; font-size: 12pt; width: 750px; margin-bottom: 10px;  }
.check{ height: 16px; width: 16px; border-radius:5px; cursor: pointer; color:#595959}
#btn_submit {background-image:url(../images/custom.png); width:76px; height:28px; border:none; opacity:1; cursor:pointer; color:#ffffff; font-weight:bold; padding-top:10px; padding-left: 28px; font-size:15px; display:inline-block; text-decoration:none; text-align: center; }
#btn_submit:hover {  color:#000000; }
#btn_submit.disabled,#btn_submit.disabled:hover { opacity:0.5; cursor:default; color:#ffffff; }
#lower_text{ width:960px; height: 46px; text-align: center; font-size: 13pt; margin-top: -30px; font-family:Avenir Next LT Pro Regular; background-color: #f2982e; line-height: 1.3; padding:3px; }

.resultbg { background:#202e3f; opacity: 0.3; width:100%; height:100%; position:absolute; z-index:10001; top:0px; left:0px; display:none; }
.feedback { z-index: 9999999; width:402px; height:auto; position:absolute; top:200px; left:300px; display:none; line-height:1.3; background-color: #ffffff;  border-top:solid 2px #4ab559; padding: 15px;} 
.feedback .fbcnt { height:100px; font-size:9pt; margin:20px 0px; }
.feedback .btnset { text-align:center; }
.feedback .btn { float:none; width:60px; margin-top:-80px; margin-left:176px; }

