.btn, .btn:visited, .btn:active, .btndisabled
{ 
    background-color:transparent !Important;
    background-image:url(../images/cnt_btn_new.png);
    width:131px;
    height:39px;
    font-family: Avenir Next LT Pro Regular;
    color: #ffffff;
    font-size: 16px;
    text-decoration: none;
    border:0px;
    cursor:pointer;
    font-weight:bold;
    padding-right: 20px;
}
.btn:hover { background: url(../images/cnt_btn_new.png) 0px 0px no-repeat; color:#000000; }
.btn1, .btn1:visited, .btn1:active, .btn1disabled
{ 
    background-color:transparent !Important;
    background-image:url(../images/custom.png);
    width:132px;
    height:39px;
    font-family: Avenir Next LT Pro Regular;
    color: #ffffff;
    font-size: 16px;
    text-decoration: none;
    border:0px;
    cursor:pointer;
    padding: 0px 4px 1px 5px;
    font-weight:bold;
}
.btn1:hover { background: url(../images/custom.png) 0px 0px no-repeat; color:#000000; }
.btn1disabled, .btn1disabled:visited, .btn1disabled:active, .btn1disabled:hover { background:url(../images/custom.png) 0px 0px no-repeat; color:#ffffff; cursor:default; opacity: 0.5;}
